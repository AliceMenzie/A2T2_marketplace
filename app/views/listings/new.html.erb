<%= form_with model: @listing, local: true do |form|  %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :category %>
<%= form.text_field :category %>
<%= form.label :description %>
<%= form.text_area :description%>
<%= form.label :price %>
<%= form.number_field :price, min: 1.00, step: 0.05 %>
<%= form.submit 'Add Product'  %>
<% end %>


