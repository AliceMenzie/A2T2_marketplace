 <div class="container">
 
  <div class="row row-cols-auto">
  <% @listings.each do |listing| %>
    <div class="col"> 
      <ul>
        <li><%=listing.name %></li>
        <li><%=listing.description %></li>
	      <li><%=listing.category %></li>
        <li><%=number_to_currency(listing.price) %></li>
       <li><%= link_to 'View Product', listing_path(listing) %></li>
      </ul>
    
      
  </div>

  <% end %>
</div>


<%= link_to 'Create New Product', new_listing_path  %>